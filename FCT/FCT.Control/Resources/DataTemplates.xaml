<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ViewModels="clr-namespace:FCT.Control.ViewModels"
        xmlns:Views="clr-namespace:FCT.Control.Views" >

    <DataTemplate DataType="{x:Type ViewModels:CarSelectionViewModel}">
        <Views:CarSelectionView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:FileMenuViewModel}">
        <Views:FileMenuView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:MainTabViewModel}">
        <Views:MainTabView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:FuelConsumptionViewModel}">
        <Views:FuelConsumptionView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:StatisticsViewModel}">
        <Views:StatisticsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:CarDataViewModel}">
        <Views:CarDataView/>
    </DataTemplate>
</ResourceDictionary>
